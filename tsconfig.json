{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": ["./src/*"],
      "@common/*": ["./src/common/*"],
      "@lib/*": ["./src/lib/*"],
      "@modules/*": ["./src/modules/*"]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  }
}
